{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": null,
    "noEmit": true,
    "composite": false,
    "paths": {
      "@antv/infographic": ["../src/index"],
      "@@/*": ["./*"],
      "@/*": ["../src/*"]
